    "test:e2e": "./node_modules/.bin/mocha --require babel-register src/test/e2e/*.test.js"
  },
  "devDependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.33.2",
    "eslint": "^4.11.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-sourcemaps": "^2.4.1",
    "he": "^1.1.1",
    "mocha": "^4.0.1",
    "selenium-webdriver": "^3.6.0",
    "sinon": "^4.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "todomvc-app-css": "^2.0.6"
  }
