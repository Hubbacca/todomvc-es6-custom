{
  "name": "todomvc-es6-custom",
  "version": "0.1.0",
  "description": "Simple TodoMVC Application with vanilla ES6",
  "author": "Ueli Kunz",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/gulp",
    "test": "npm run test:unit && npm run test:e2e",
